<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              style="@style/listview_bg_item"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/bg_app"
              android:orientation="vertical" >


    <!--铺货状态-->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        >

        <ImageView
            android:id="@+id/zdzs_caculate_point1"
            style="@style/point"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:src="@drawable/ico_point"/>

        <TextView
            android:id="@+id/item_xt_caculate_tv_indexvalue"
            style="@style/page_label_big"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/zdzs_caculate_point1"
            android:text="铺货状态"/>

    </RelativeLayout>


    <et.tsingtaopad.view.NoScrollListView
        android:id="@+id/item_xt_caculate_lv_indexvalue"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="@color/listview_line"
        android:layout_marginTop="14dp"
        android:layout_marginBottom="14dp"
        android:dividerHeight="1dp" />





</LinearLayout>